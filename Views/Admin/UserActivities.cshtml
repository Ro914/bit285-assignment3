@model bit285_assignment3_api.ViewModels.UserAdmin
@addTagHelper "*, Microsoft.AspNetCore.Mvc.TagHelpers"
@{
    Layout = "_Layout";
    ViewBag.Title = "User Activities";
}
<h1>User Activities</h1>
<h2>@Model.FullName</h2>
<table>
    <thead><tr><th>Date</th><th>Activity</th></tr></thead>
    <tbody>
        @foreach (bit285_assignment3_api.Models.Activity a in Model.Activities)
        {
            <tr><td>@a.ActivityDate</td><td>@a.Type</td></tr>
        }
    </tbody>
</table>
<br/>
<a asp-action="Index">Return to user list</a>